- if @enterpsises.any?
  .row.full-width
    .large-4.columns
      - @enterpsises.each do |enterprise|
        .search-result
          .large-12.columns
            = link_to enterprise.name, enterprise_path(enterprise)
            %span.secondary-info= "#{enterprise.city}, #{enterprise.state_name}"
    .large-8.columns
      = gmaps4rails(@enterprises_json)

- else
  .row
    .large-12.large-centered.columns
      .centered
        = "Nothing foud that matches your search criteria"
        = link_to "Try again...", new_landing_page_path